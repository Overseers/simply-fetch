{
  "name": "simply-fetch",
  "version": "0.0.1",
  "description": "Fetch wrapper to simplify the interface and the output.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-s clean build:prod",
    "build:prod": "tsc",
    "build:dev": "tsc -w",
    "clean": "rimraf dist/",
    "npm:version:patch": "npm version patch --no-git-tag-version",
    "npm:version:minor": "npm version patch --no-git-tag-version",
    "npm:version:major": "npm version patch --no-git-tag-version",
    "npm:publish": "npm publish",
    "publish": "run-s build:prod npm:version:bump npm:publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Overseers/simply-fetch.git"
  },
  "author": "CrypticSafe5",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Overseers/simply-fetch/issues"
  },
  "homepage": "https://github.com/Overseers/simply-fetch#readme",
  "dependencies": {
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  }
}
